{
  "platform": {
    "apiRuntime": "node:18"
  },
  "navigationFallback": {
    "rewrite": "/"
  },
  "responseOverrides": {
    "404": {
      "statusCode": 404,
      "redirect": "/"
    }
  },
  "globalHeaders": {
    "Access-Control-Allow-Origin": "*",
    "Access-Control-Allow-Methods": "GET"
  }
}