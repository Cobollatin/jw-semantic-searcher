{
  "$schema": "https://aka.ms/azure/static-web-apps-cli/schema",
  "configurations": {
    "semantic-searcher": {
      "appLocation": "../webapp",
      "apiLocation": "../api",
      "apiPort": 7071,
      "outputLocation": "dist/semantic-searcher",
      "apiLanguage": "node",
      "apiVersion": "18",
      "apiBuildCommand": "npm run build",
      "appBuildCommand": "npm run build",
      "printConfig": "true",
      "swaConfigLocation": "./staticwebapp.config.json",
      "verbose": "log"
    }
  }
}